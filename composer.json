{
    "name": "imerfanahmed/wp-log-lens",
    "description": "This is my package wp-log-lens",
    "keywords": [
        "imerfanahmed",
        "wp-log-lens"
    ],
    "homepage": "https://github.com/imerfanahmed/wp-log-lens",
    "license": "MIT",
    "authors": [
        {
            "name": "Erfan Ahmed Siam",
            "email": "erfan.siam98@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Imerfanahmed\\WpLogLens\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imerfanahmed\\WpLogLens\\Tests\\": "tests"
        }
    },
    "scripts": {
            "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
