{
    "name": "imerfanahmed/wp-log-lens",
    "description": "This is my package wp-log-lens",
    "keywords": [
        "imerfanahmed",
        "wp-log-lens"
    ],
    "homepage": "https://github.com/imerfanahmed/wp-log-lens",
    "license": "MIT",
    "authors": [
        {
            "name": "Erfan Ahmed Siam",
            "email": "erfan.siam98@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Imerfanahmed\\WpLogLens\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imerfanahmed\\WpLogLens\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
